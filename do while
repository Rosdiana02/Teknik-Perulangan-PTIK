include <iostream>
include <conio.h>
using namespace std;
int main()
{
  int bil=10;
  do
  {
    cout <<"HI! do-while ini tetap dijalankan, walaupun kondisi / keadaan salah."<< endl;
  }
  while (bil < 5);
    cout<< endl;
    cout<< "Keluar. Bukan pernyataan do-while."<< endl;
    _getche ();
    return 0;
}
